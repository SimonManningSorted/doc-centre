{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "pro/src/**.csproj"
          ]
        }
      ],
      "dest": "pro/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "pro/api/**.yml",
          "pro/api/index.md"
        ]
      },
      {
        "files": [
          "pro/reports/**.md",
          "pro/reports/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      { 
        "files": "pro/fusion/**"
      }
    ],
    "resource": [
      {
        "files": [
          "pro/images/**"
        ]
      },
      {
        "files": [
          "myfavicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "pro/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appFaviconPath": "/myfavicon.ico"
    }, 
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/sorted-styles"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}