{
  "build": {
    "content": [{
        "files": [
          "pro/api/help/**.yml",
          "pro/api/help/**.md",
          "pro/toc.yml",
          "pro/index.md"
        ]
      },
      {
        "files": [
          "pro/reports/**.md",
          "pro/reports/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [{
        "files": [
          "pro/images/**",
          "images/**"
        ]
      },
      {
        "files": [
          "pro/scripts/**"
        ]
      },
      {
        "files": [
          "myfavicon.ico"
        ]
      }
    ],
    "overwrite": [{
      "files": [
        "pro/apidoc/**.md"
      ],
      "exclude": [
        "obj/**",
        "_site/**"
      ]
    }],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/sorted-styles"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}