{
  "build": {
    "content": [{
        "files": [
          "pro/api/help/**.yml",
          "pro/api/help/**.md",
          "pro/api/shipments/**.yml",
          "pro/api/shipments/**.md",          
          "pro/toc.yml",
          "pro/index.md"
        ]
      },
      {
        "files": [
          "pro/reports/**.md",
          "pro/reports/toc.yml",
          "pro/overview/**.md",
          "pro/overview/toc.yml",          
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [{
        "files": [
          "pro/images/**",
          "images/**"
        ]
      },
      {
        "files": [
          "pro/scripts/**"
        ]
      },
      {
        "files": [
          "myfavicon.ico"
        ]
      }
    ],
    "overwrite": [{
      "files": [
        "pro/apidoc/**.md"
      ],
      "exclude": [
        "obj/**",
        "_site/**"
      ]
    }],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/sorted-styles"
    ],
    "postProcessors": [ "ExtractSearchIndex", "AuthorsProcessor" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© Sorted Group 2020. Generated by <strong>DocFX</strong></span>",
      "_appTitle": "Sorted Docs",
      "_enableSearch": true
    }
  }
}